{
    "manifest_version": 3,
    "name": "AFM Autoload Bootstrapper",
    "version": "1.0",
    "description": "Автоматическая загрузка и выполнение внешнего скрипта для автозаполнения на websfm.kz",
    "host_permissions": [
        "https://websfm.kz/form-fm/*",
        "https://raw.githubusercontent.com/*"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://websfm.kz/form-fm/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}